! vim: set filetype=xdefaults :

urxvt.font: xft:Inconsolata\ for\ Powerline:pixelsize=15
urxvt.letterSpace: -1
urxvt.scrollBar: false
*.cursorBlink: true

Xft.dpi: 168
!Xft.dpi: 157 actual screen dpi ?
!Xft.dpi: 144 is nearest multiple of 96
!Xft.dpi: 168 is next multiple of 96
!Xft.dpi: 97

! From archwiki on font config
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

